{
    "extends": "./tsconfig.json",
    "include": [
        "**/*.ts",
        "**/*.tsx",
        "**/*.js",
        "**/*.jsx",
        "next-env.d.ts"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "storybook-static",
        "coverage",
        "jest.config.js",
        "**/*.test.tsx",
        "tests",
        "**/*.stories.mdx",
        "**/*.stories.@(js|jsx|ts|tsx)"
    ]
}