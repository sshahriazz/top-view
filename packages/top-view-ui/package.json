      "ts-jest": "^26.5.5",
      "typescript": "4.7.4",
      "webpack": "^5.53.0",
      "webpack-bundle-analyzer": "^4.4.2",
      "webpack-cli": "^3.3.11",
      "webpack-merge": "^5.8.0"
    }
